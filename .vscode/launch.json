{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/BinomialPricingModel",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "build",
            "miDebuggerArgs": "",
            "logging": {
                "moduleLoad": false,
                "trace": true,
                "engineLogging": false
            },
            "serverStarted": "",
            "filterStderr": true,
            "filterStdout": true,
            "sourceFileMap": {
                "/usr/src/linux": "${workspaceFolder}/src"
            },
            "windows": {
                "miDebuggerPath": "C:\\msys64\\ucrt64\\bin\\g++.exe"
            }
        }
    ]
}
